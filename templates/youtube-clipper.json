{
  "schemaVersion": "0.2.0",
  "name": "YouTube",
  "behavior": "create",
  "noteContentFormat": "![{{schema:name}}]({{url}})\n\n# Summary\n\n# Concepts",
  "properties": [
    {
      "name": "title",
      "value": "{{schema:name}}",
      "type": "text"
    },
    {
      "name": "author",
      "value": "{{schema:author|wikilink}}",
      "type": "multitext"
    },
    {
      "name": "published",
      "value": "{{schema:uploadDate|date:YYYY-MM-DD}}",
      "type": "date"
    },
    {
      "name": "source",
      "value": "{{url}}",
      "type": "text"
    },
    {
      "name": "image",
      "value": "{{schema:thumbnailUrl|slice:0}}",
      "type": "text"
    },
    {
      "name": "created",
      "value": "{{date}}",
      "type": "date"
    },
    {
      "name": "tags",
      "value": "videos, clippings",
      "type": "multitext"
    }
  ],
  "triggers": ["https://www.youtube.com/watch?v="],
  "noteNameFormat": "{{schema:author}} â€“ {{schema:name}}",
  "path": "_literature/_youtube/"
}
